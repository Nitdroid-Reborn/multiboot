#
# Power management configuration file
#

# Power save config
enable_off_mode 1
sleep_while_idle 1
clocks_off_while_idle 1
voltage_off_while_idle 1

# Automatic voltage scaling
scaling_governor ondemand
scaling_max_freq 900000
scaling_min_freq 250000

# DVFS load treshold
up_threshold 75

# DVFS sampling rate
sampling_rate 300000

# DVFS operating points
vdd1_opps_vsel 30 30 30 30 33 38 45 45 54 54 60 64 68 72 72
dsp_opps_rate 90 90 180 360 400 430 430 430 430 500 500 500 500 520 520

# Freqs NOT to use
avoid_frequencies 850000 805000 700000 550000 125000 

# Smartreflex
smartreflex_vdd1 0
smartreflex_vdd2 0
